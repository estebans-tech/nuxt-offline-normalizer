{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
      "#imports": ["./.nuxt/imports.d.ts"]
    },
    "moduleResolution": "bundler",
    "types": ["node", "vite/client", "nuxt"]
  },
  "include": ["env.d.ts", "composables", "workers", "pages", "components", "server", "app.vue"],
  "exclude": ["dist", ".output", "node_modules"]
}
